CT_EXPERIMENTAL=y
CT_PREFIX_DIR="${HOME}/x-tools/${CT_TARGET}"
CT_PATCH_BUNDLED_LOCAL=y
CT_LOCAL_PATCH_DIR="${CT_LIB_DIR}/local-patches"
CT_LOCAL_TARBALLS_DIR="${CT_TOP_DIR}/src"
CT_STRIP_HOST_TOOLCHAIN_EXECUTABLES=y
CT_STRIP_TARGET_TOOLCHAIN_EXECUTABLES=y
CT_SAVE_TARBALLS=y
CT_TARGET_CFLAGS="-mlongcalls"
CT_ARCH_xtensa=y
CT_XTENSA_CUSTOM=y
CT_ARCH_XTENSA_CUSTOM_NAME="lx106"
CT_ARCH_XTENSA_CUSTOM_OVERLAY_LOCATION="${CT_LIB_DIR}/overlays"
CT_TARGET_VENDOR="lx106"
CT_LIBC_NEWLIB_V_2_0_0=y
CT_LIBC_NEWLIB_TARGET_CFLAGS="-DMALLOC_PROVIDED"
CT_LIBC_NEWLIB_DISABLE_SUPPLIED_SYSCALLS=y
CT_CC_LANG_CXX=y
CT_CC_GCC_V_5_4_0=y
CT_BINUTILS_VERSION="2.25.1"
CT_BINUTILS_V_2_25_1=y
CT_BINUTILS_2_25_1_or_later=y
CT_BINUTILS_2_25_or_later=y
CT_CC_GCC_ENABLE_CXX_FLAGS="-fno-exceptions -fno-rtti"
# CT_CC_CXA_ATEXIT is not set
# CT_CC_GCC_STATIC_LIBSTDCXX is not set
# CT_CC_GCC_LDBL_128 is not set
# CT_INSTALL_DIR_RO is not set
CT_DEBUG_gdb=y
